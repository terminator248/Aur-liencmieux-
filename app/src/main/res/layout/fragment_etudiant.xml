<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Etudiant">

    <!-- TODO: Update blank fragment layout -->

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/actualites"
            android:layout_width="259dp"
            android:layout_height="59dp"
            android:text="Actualités"
            android:textColor="#FFFFFF"
            tools:ignore="HardcodedText,MissingConstraints"
            tools:layout_editor_absoluteX="76dp"
            tools:layout_editor_absoluteY="237dp" />

        <Button
            android:id="@+id/etudes"
            android:layout_width="162dp"
            android:layout_height="48dp"
            android:text="Etudes"
            android:textColor="#FFFFFF"
            tools:ignore="HardcodedText,MissingConstraints"
            tools:layout_editor_absoluteX="124dp"
            tools:layout_editor_absoluteY="341dp" />

        <Button
            android:id="@+id/vos_etudes"
            android:layout_width="156dp"
            android:layout_height="57dp"
            android:text="Vos études"
            android:textColor="#FFFFFF"
            tools:ignore="HardcodedText,MissingConstraints"
            tools:layout_editor_absoluteX="127dp"
            tools:layout_editor_absoluteY="597dp" />

        <ImageView
            android:id="@+id/imagelogo"
            android:layout_width="98dp"
            android:layout_height="87dp"
            android:contentDescription="@drawable/logoandroid"
            android:src="@drawable/logoandroid"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="30dp"
            tools:layout_editor_absoluteY="36dp" />

        <Button
            android:id="@+id/bouton_profil"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/bouton_rond"
            android:text="Profil"
            android:textColor="#FFFFFF"
            android:textSize="18sp"
            tools:ignore="HardcodedText,MissingConstraints"
            tools:layout_editor_absoluteX="279dp"
            tools:layout_editor_absoluteY="46dp" />


    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>